killaura=main:Tab({
    Name="Kill Aura"
})
killauraon=false
stoploop=false
killauradst=25
while not stoploop and task.wait() do
    if not killauraon then
        return
    end
    for i,v in getplrs() do
        attack(v,killauradst)
    end
end
ui.OnDestruction:Connect(function()
    killauraon=false
    stoploop=true
end)
killauradistance=killaura:TextBox({
    Name="set killaura max distance",
    Callback=function(a)
        killauradst=a
    end,
    Default="25",
    OnlyNumbers=true
})
killauratoggle=killaura:Toggle({
    Name="Kill Aura",
    Callback=function(a)
    killauraon=a
})
