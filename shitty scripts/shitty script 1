tpwait=0.3
main=ui:Window({
    Title="Shitty Script",
    Description="Self Explanatory",
    HideBind=Enum.KeyCode.KeypadZero
})
teleports=main:Tab({
    Name="Teleports"
})
tpwaitbutton=teleports:TextBox({
    Name="Set Teleport Wait (0.3 recommended)",
    Callback=function(a)
        tpwait=a
    end,
    Default="0.3",
    OnlyNumbers=true
})
