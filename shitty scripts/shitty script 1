towntpnames={"lmaoshittyplaceholder"}
for i,v in towntps do
    table.insert(towntpnames,i)    
end
towntpsfr=teleports:Dropdown({
    Name="Town Teleports",
    Items=towntpnames,
    Callback=function(v)
        local pos=towntps[v]
        tp(pos.X,pos.Y,pos.Z,tpwait)
    end,
    AlwaysOpen=true,
    FLAG="towntpflag"

})
ui.flags.towntpflag.OnChange:Connect(function()
    task.wait()
    towntpsfr:SelectItem("lmaoshittyplaceholder")
end)
