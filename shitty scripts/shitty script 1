function fti(a,b)
    firetouchinterest(a,b,0)
    firetouchinterest(a,b,1)
end
function tp(x,y,z,wt)
    local plr=game.Players.LocalPlayer
    local chr=plr.Character or plr.CharacterAdded:Wait()
    local pp=chr.PrimaryPart or chr.HumanoidRootPart
    local prt
    if not pp then
        print("no pp")
        return("no pp")
    end
    if plr.Team=="Doctor" then
        prt=workspace:FindFirstChild("GUARDONLY",true)
    else
        prt=workspace:FindFirstChild("DOCONLY",true)
    end
    fti(pp,prt)
    task.wait(wt)
    pp.CFrame=CFrame.new(x,y,z)
    task.wait()
    prt.CFrame=CFrame.new(400,600,400)
end
function getplrs()
    local plrs={}
    for i,v in game.Players:GetChildren() do
        table.insert(plrs,v)
    end
    return plrs
end
function getchr(plr)
    local chr = v.Character or v.CharacterAdded:Wait()
    local hum=chr.Humanoid or nil
    if not chr or not hum or not hum.Health>=0 then
        return false
    end
    if chr and hum and hum.Health>=0 then
        return chr
    end
end
function getparts(parent)
    local parts={}
    for i,v in parent do
        for i,v in v:GetChildren() do
            if v:IsA("BasePart") then
                table.insert(parts,v)
            end
        end
    end
    return parts
end
function checkdst(a,b,num)
    local dst
    dst=(a.Position-b.Position).Magnitude
    if dst>num then
        return false
    end
    return true
end
function attack(plr,dst)
    local plr2=game.Players.LocalPlayer
    local chr=getchr(plr2)
    local tool=chr:FindFirstChildWhichIsA("Tool")
    local toolpart=chr:FindFirstChildWhichIsA("TouchTransmitter",true).Parent
    if not (chr or tool or toolpart) then
        return
    end
    for i,v in getparts(getchr(plr)) do
        if not checkdis(toolpart,v,dst) then
            return
        end
        tool:Activate()
        fti(toolpart,v)
    end
end
